                    <btnLigacao
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Caminho/ ligação do botão"
                        linkPattern="\{0}.html"
                        name="./btnLigacao"
                        xtype="pathfield"/>
                </items>
            </geral>
        </items>
    </items>
